    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.99.0/css/materialize.css">
  </head>
  <body>

<div class="container">
  <h1><%= title %> Links</h1>
  <button id="signOut" type="button" onclick="signOut();">Sign Out</button>
  <button><a href="/home">Home</a></button>
  <p> * If link takes too long to load (30s), refresh the page and choose another link.</p>
  <ul class="collection width-header" id="showsUl">
    <% seriesShowEpLinklist.forEach(function(show) { %>
      <li id="ep" class='collection-item'>
        <a href="/link/<%= title %>/<%= show.linkID %>" title="Link <%= show.linkNum %>" target="_blank">Link <%= show.linkNum %></a>
      </li>
    <% }) %>
  </ul>

</div>

</body>

